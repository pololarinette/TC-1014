#PROGRAM 1
import math
def distance(x1,y1,x2,y2):
    dist = math.hypot(x2 - x1, y2- y1)
    print(dist)

x1 = int(input("x1"))
y1 = int(input("y1"))
x2 = int(input("x2"))
y2 = int(input("y2"))

distance(x1,y1,x2,y2)

#PROGRAM 2
n=int(input("Give me a number and I will give you its fibonacci number"))

def fib(n):
 a,b = 1,1
 for i in range(n-1):
  a,b = b,a+b
 return a
print (fib(n))
